<content xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <templateSets>
        <templates-apps xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:jnt="http://www.jahia.org/jahia/nt/1.0"
                        xmlns:jmix="http://www.jahia.org/jahia/mix/1.0" xmlns:j="http://www.jahia.org/jahia/1.0"
                        xmlns:mix="http://www.jcp.org/jcr/mix/1.0" jcr:mixinTypes="jmix:sitemap" j:siteType="templatesSet"
                        jcr:primaryType="jnt:virtualsite" changefreq="monthly" j:installedModules="templates-apps"
                        priority="0.5">
            <home j:displayInMenu="primary" jcr:primaryType="jnt:page"
                  j:templateNode="/templateSets/templates-apps/templates/base/home">
                <j:translation_en jcr:title="Home" jcr:language="en" jcr:primaryType="jnt:translation"/>
                <listA jcr:primaryType="jnt:contentList"/>
                <listB jcr:primaryType="jnt:contentList"/>
            </home>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder">
                <themes jcr:primaryType="jnt:folder">
                    <default jcr:primaryType="jnt:folder"/>
                </themes>
            </files>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder"/>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <base jcr:mixinTypes="jmix:renderable" j:view="apps" jcr:primaryType="jnt:template">
                    <logo jcr:primaryType="jnt:contentList"/>
                    <pagetitle jcr:primaryType="jnt:contentList"/>
                    <simpleSearch jcr:primaryType="jnt:contentList"/>
                    <topMenu jcr:primaryType="jnt:contentList"/>
                    <topfooter jcr:primaryType="jnt:contentList"/>
                    <footer jcr:primaryType="jnt:contentList">
                        <footer-row column="1col16" jcr:primaryType="jnt:row">
                        </footer-row>
                    </footer>
                    <footer-row-col1 jcr:primaryType="jnt:contentList">
                        <footer j:allowedTypes="jnt:bigText" jcr:primaryType="jnt:absoluteArea"/>
                    </footer-row-col1>
                    <header jcr:primaryType="jnt:contentList">
                        <header-row column="1col16" jcr:primaryType="jnt:row">
                        </header-row>
                    </header>
                    <header-row-col1 jcr:primaryType="jnt:contentList">
                        <header j:allowedTypes="" jcr:primaryType="jnt:absoluteArea"/>
                    </header-row-col1>
                    <header-row-col2 jcr:primaryType="jnt:contentList"/>
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <pagecontent jcr:primaryType="jnt:area"/>
                    </pagecontent>
                    <home jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <row column="2col115" jcr:primaryType="jnt:row">
                            </row>
                        </pagecontent>
                        <row-col2 jcr:primaryType="jnt:contentList">
                            <login jcr:primaryType="jnt:loginForm" j:displayRememberMeButton="false"
                                   j:displayLoginButton="false">
                                <j:translation_en jcr:title="Login" jcr:language="en"
                                                  jcr:primaryType="jnt:translation"/>
                            </login>
                            <listB j:allowedTypes="" jcr:primaryType="jnt:area"/>
                        </row-col2>
                        <row-col1 jcr:primaryType="jnt:contentList">
                            <listA j:allowedTypes="" jcr:primaryType="jnt:area"/>
                        </row-col1>
                    </home>
                    <folderTemplate jcr:primaryType="jnt:contentTemplate" j:applyOn="jnt:folder jnt:contentFolder" j:hiddenTemplate="false" j:defaultTemplate="true">
                      <pagecontent jcr:primaryType="jnt:contentList">
                        <facets jcr:primaryType="jnt:facets" j:bindedComponent="/templateSets/templates-apps/templates/base/folderTemplate/pagecontent/mainResourceDisplay">
                          <fieldFacet mincount="1" field="jmix:tagged;j:tags" jcr:primaryType="jnt:fieldFacet" missing="false" facet="tags" offset="0" limit="100">
                            <j:translation_en jcr:language="en" label="Tags" jcr:primaryType="jnt:translation" />
                          </fieldFacet>
                        </facets>
                        <pager pageSize="10" jcr:primaryType="jnt:pager" j:bindedComponent="/templateSets/templates-apps/templates/base/folderTemplate/pagecontent/mainResourceDisplay" displayPager="true" />
                        <mainResourceDisplay j:canOrderInContribution="true" jcr:mixinTypes="jmix:contributeMode" j:mainResourceView="table" j:canDeleteInContribution="true" jcr:primaryType="jnt:mainResourceDisplay" j:contributeTypes="jnt:contentFolder jmix:editorialContent" j:editableInContribution="true" />
                      </pagecontent>
                    </folderTemplate>
                    <editContent j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default" j:requiredMode="contribute" jcr:primaryType="jnt:contentTemplate" j:hiddenTemplate="true" j:applyOn="jnt:content">
                      <pagecontent jcr:primaryType="jnt:contentList">
                        <mainResourceDisplay-table j:mainResourceView="contribute.table" jcr:primaryType="jnt:mainResourceDisplay" />
                        <tabularList jcr:primaryType="jnt:tabularList">
                          <content jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Content" jcr:primaryType="jnt:translation" />
                            <mainResourceDisplay-edit j:mainResourceView="contribute.edit" jcr:primaryType="jnt:mainResourceDisplay" />
                          </content>
                          <metadata jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Metadata" jcr:primaryType="jnt:translation" />
                            <mainResourceDisplay-metadata j:mainResourceView="contribute.editmetadata" jcr:primaryType="jnt:mainResourceDisplay" />
                          </metadata>
                          <tags jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Tags" jcr:primaryType="jnt:translation" />
                            <tag jcr:primaryType="jnt:tagging" j:bindedComponent="/templateSets/templates-apps/templates/base/editContent/pagecontent/mainResourceDisplay-table">
                              <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Tag" jcr:primaryType="jnt:translation" />
                            </tag>
                            <addTag jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:tagging" j:bindedComponent="/templateSets/templates-apps/templates/base/editContent/pagecontent/mainResourceDisplay-table" j:view="addtag">
                              <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Tag" jcr:primaryType="jnt:translation" />
                            </addTag>
                          </tags>
                          <categories jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Classifications" jcr:primaryType="jnt:translation" />
                            <category jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:categorizing" j:view="default" j:bindedComponent="/templateSets/templates-apps/templates/base/editContent/pagecontent/mainResourceDisplay-table">
                              <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Category" jcr:primaryType="jnt:translation" />
                            </category>
                            <category-1 jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:categorizing" j:bindedComponent="/templateSets/templates-apps/templates/base/editContent/pagecontent/mainResourceDisplay-table" j:view="addcategory">
                              <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Category" jcr:primaryType="jnt:translation" />
                            </category-1>
                          </categories>
                        </tabularList>
                      </pagecontent>
                    </editContent>
                      <viewContent j:requiredMode="normal" jcr:primaryType="jnt:contentTemplate" j:applyOn="jnt:content" j:hiddenTemplate="true">
                        <pagecontent jcr:primaryType="jnt:contentList">
                          <mainResourceDisplay-table j:mainResourceView="contribute.table" jcr:primaryType="jnt:mainResourceDisplay" />
                          <mainResourceDisplay j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay" />
                        </pagecontent>
                      </viewContent>
                    <fileTemplate jcr:primaryType="jnt:contentTemplate" j:applyOn="jnt:file" j:defaultTemplate="true">
                      <pagecontent jcr:primaryType="jnt:contentList">
                        <mainResourceDisplay-table j:mainResourceView="contribute.table" jcr:primaryType="jnt:mainResourceDisplay" />
                        <mainResourceDisplay j:mainResourceView="detail" jcr:primaryType="jnt:mainResourceDisplay" />
                      </pagecontent>
                    </fileTemplate>

                </base>
            </templates>
        </templates-apps>
    </templateSets>
</content>